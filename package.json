{
  "name": "@james-marriott/personal_website",
  "version": "0.0.1",
  "description": "James Marriott Personal Website",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesmarriott/personal_website"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "eslint": "^v8.0.0",
    "js-yaml": "^3.14.0",
    "postcss-cli": "^7.0.0",
    "prettier": "^2.1.2"
  },
  "scripts": {
    "autoprefixer": "postcss src/css/**/*.css src/modules/**/*.css !src/**/_layout.css !src/**/_normalize.css !src/**/_reset.css !src/**/_macros.css !src/**/main.css !src/**/theme-overrides.css --use autoprefixer --replace --no-map",
    "eslint": "eslint src",
    "fetch": "hs fetch james-marriott-personal-website-theme src",
    "lint": "eslint src && prettier --list-different 'src/**/*.js' 'src/**/*.json'",
    "prettier:write": "prettier --write 'src/**/*.js' 'src/**/*.json'",
    "upload": "hs upload src james-marriott-personal-website-theme",
    "watch": "hs watch src james-marriott-personal-website-theme"
  },
  "browserslist": [
    "defaults",
    "IE 11"
  ]
}
